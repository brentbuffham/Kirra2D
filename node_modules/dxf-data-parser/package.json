{
	"name": "dxf-data-parser",
	"version": "1.3.8",
	"description": "Parse dxf files into a readable, logical js object.",
	"main": "./dist/dxf-parser.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "mocha --require @babel/register test",
		"dev": "tsc -w & webpack --mode development",
		"entities": "node samples/node/fetch-entities.js",
		"parse": "node samples/node/parse-sync.js",
		"clean": "node samples/node/clean-dxf.js",
