		"@types/lodash": "^4.14.178",
		"approvals": "^3.0.5",
		"mocha": "^8.0.1",
		"should": "^13.2.3",
		"typescript": "^4.4.3",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"just-diff": "^5.0.1",
		"lodash": "^4.17.21",
		"loglevel": "^1.7.1"
